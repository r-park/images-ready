{
  "name": "imagesready",
  "version": "0.1.0",
  "description": "ImagesReady is a lightweight utility for detecting when images have been loaded. It supports promises and can be used with or without jQuery.",
  "homepage": "https://github.com/r-park/images-ready",
  "repository": "https://github.com/r-park/images-ready.git",
  "author": "Richard Park",
  "license": "MIT",

  "devDependencies": {
    "jquery": "*"
  },

  "ignore": [
    "**/.*",
    "gulpfile.js",
    "node_modules",
    "package.json",
    "src",
    "target",
    "tmp",
    "vendor"
  ],

  "keywords": [
    "images",
    "imagesready",
    "imagesloaded",
    "load",
    "onload"
  ],

  "moduleType": [
    "amd",
    "globals",
    "node"
  ]
}
